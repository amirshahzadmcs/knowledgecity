# Root-level ignore for Laravel + React

# Laravel backend
/backend/vendor/
/backend/node_modules/
/backend/public/storage
/backend/storage/*.key
/backend/.env
/backend/.env.*.backup
/backend/.phpunit.result.cache
/backend/phpunit.xml
/backend/homestead.yaml
/backend/Homestead.json
/backend/.idea
/backend/.vscode
/backend/.DS_Store

# Laravel IDE helper files
/backend/_ide_helper.php
/backend/_ide_helper_models.php
/backend/.phpstorm.meta.php

# React frontend
/frontend/node_modules/
/frontend/.env
/frontend/.DS_Store
/frontend/build/
/frontend/dist/
/frontend/.next/        # if using Next.js
/frontend/.parcel-cache/ # if using Parcel

# Logs
**/npm-debug.log*
**/yarn-debug.log*
**/yarn-error.log*
**/pnpm-debug.log*